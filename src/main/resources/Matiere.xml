<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.iscae.Planification_Emploi_Temps.data.Models.Matiere">
        <table name="matiere" schema="emploi" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="nom">
                <column name="nom" length="50"/>
            </basic>
            <basic name="heures">
                <column name="heures" precision="-1"/>
            </basic>
            <basic name="filiere">
                <column name="filiere" precision="-1"/>
            </basic>
            <many-to-one name="filiereByFiliere" optional="true"
                         target-entity="com.iscae.Planification_Emploi_Temps.data.Models.Filiere">
                <join-column name="filiere" referenced-column-name="id"/>
            </many-to-one>
            <one-to-many name="coursById" mapped-by="matiereByMatiere"
                         target-entity="com.iscae.Planification_Emploi_Temps.data.Models.Cours"/>
        </attributes>
    </entity>
</entity-mappings>
